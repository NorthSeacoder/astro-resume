---
const PUBLIC_UMAMI = import.meta.env.PUBLIC_UMAMI
const isProduction = import.meta.env.MODE === 'production';
---

{
    isProduction && PUBLIC_UMAMI ? (
        <script
            is:inline
            async
            defer
            data-website-id={PUBLIC_UMAMI}
            src='https://umami.mengpeng.tech/script.js'
        />
    ) : null
}
